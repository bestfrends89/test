FROM liveslak/slackware:base_x64_15.0
RUN mkdir /app && chmod 777 /app
WORKDIR /app
COPY . /app

RUN     curl -O -L --output-dir /aa --url https://streamaa.herokuapp.com/dd/slapt-get-0.11.5-x86_64-1gv.txz; \
        installpkg /aa/slapt-get-0.11.5-x86_64-1gv.txz; \
        slapt-get -u; \
        slapt-get -i -y python3 elogind; \
        ls /var/log/packages/


    
    
    
    
    
 

